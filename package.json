{
  "name": "vkAR",
  "version": "1.0.0",
  "repository": "https://github.com/JesseTheRobot/TweetStream.git",
  "author": "JesseTheRobot",
  "license": "MIT",
  "module": "CommonJS",
  "bin": {
    "app": "./build/vkAR.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "ts-node ./src/vkAr.ts",
    "pkg": "npm run build && pkg ."
  },
  "dependencies": {
    "@bundlr-network/client": "^0.5.12",
    "axios": "^0.26.0",
    "pkg": "^5.5.2",
    "vkflow": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.18",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "pkg": {
    "assets": [
      "build/*",
      "./wallet.json",
      "./config.json"
    ],
    "targets": [
      "node16-macos-x64",
      "node16-win-x64"
    ],
    "outputPath": "dist"
  }
}
